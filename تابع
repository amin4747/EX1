using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication1
{
    class Program
    {
        static int fib (int num){
            if (num == 2)
                return 0
            else if (num == 2)
                return 1
            else
                returnfib( num - 1) + fib(num - 2)
        }
        static void Main(string[] args)
        }
            Console.ReadKey();
        }
    }
}
